{
    "name": "quick-debug",
    "displayName": "quick-debug",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Other"
    ],
    "languages": [{
        "id": "quickdebug",
        "extensions": [ ".quickdebug" ]
    }],
    "activationEvents": [
        "onStartupFinished",
        "onCommand:quickdebug.sendToDebugger"
    ],

    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "quickdebug.sendToDebugger",
                "title": "Send to Debugger"
            }
        ],
        "keybindings": [
            {
                "command": "quickdebug.sendToDebugger",
                "key": "alt+d",
                "when": "debuggersAvailable"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.48.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^14.0.27",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    }
}